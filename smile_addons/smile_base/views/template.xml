<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data noupdate="0">

    <template id="translate_progress_bar" inherit_id="web.assets_backend">
      <xpath expr="." position="inside">
        <link rel="stylesheet" href="/smile_base/static/src/css/env_ribbon.css"/>
        <script type="text/javascript" src="/smile_base/static/src/js/env_ribbon.js"></script>
        <script type="text/javascript" src="/smile_base/static/src/js/progress_bar.js"></script>
        <script type="text/javascript" src="/smile_base/static/src/js/kanban_column.js"></script>
      </xpath>
    </template>

    <template id="smile_base_css" name="smile_base_css" inherit_id="web_editor.assets_backend">
      <xpath expr="." position="inside">
        <link rel="stylesheet" href="/smile_base/static/src/css/base.css"/>
      </xpath>
    </template>

    <template id="login" inherit_id="web.login" name="Login window">
      <xpath expr="//label[@for='login']" position="replace">
        <label for="login" class="control-label">Login</label>
      </xpath>
    </template>

  </data>
</openerp>