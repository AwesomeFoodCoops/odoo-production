<?xml version="1.0"?>
<odoo>
    <!-- Views -->
    <record model="ir.ui.view" id="view_contact_us_message_wizard">
        <field name="model">contact.us.message.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="message" nolabel="1"/>
                </group>
                <footer>
                    <button string="OK" name="button_ok" type="object" default_focus="1" class="btn-primary"/>
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

