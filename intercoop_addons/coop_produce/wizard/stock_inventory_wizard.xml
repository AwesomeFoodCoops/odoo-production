<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="view_stock_inventory_wizard_form" model="ir.ui.view">
			<field name="name">stock.inventory.wizard.form</field>
			<field name="model">stock.inventory.wizard</field>
			<field name="sequence" eval="18"/>
			<field name="arch" type="xml">
				<form>
					<h2>Choose the date began order scheduling on.</h2>
					<group col="4">
						<field name="week_date" />
					</group>
					<footer>
						<button name="action_ok" string="OK" type="object" class="btn-primary" />
						<button string="Cancel" class="btn-default" special="cancel"/>
					</footer>
				</form>
			</field>
		</record>


	</data>
</openerp>
