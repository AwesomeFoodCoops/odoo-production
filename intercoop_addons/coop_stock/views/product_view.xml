<?xml version="1.0" encoding="UTF-8" ?>
<openerp>
    <data>
        <record id="view_coop_stock_product_tree" model="ir.ui.view">
            <field name="name">product.product.coop_stock.tree.inherit</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_product_tree_view"/>
            <field name="arch" type="xml">
                <field name="barcode" position="after">
                    <field name="active" />
                </field>
            </field>
        </record>

        <record id="view_coop_stock_product_template_tree" model="ir.ui.view">
            <field name="name">product.template.coop_stock.tree.inherit</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_tree_view"/>
            <field name="arch" type="xml">
                <field name="uom_id" position="after">
                    <field name="active" />
                </field>
            </field>
        </record>

    </data>
</openerp>
