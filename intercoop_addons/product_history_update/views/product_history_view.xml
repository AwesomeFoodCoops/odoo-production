<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
  <data>

    <record id="view_template_history_form" model="ir.ui.view">
        <field name="name">Product Template</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product_history.view_template_history_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='history_range']" position="after">
              <button name="update_history" string="Update History"
                      type="object"/>
            </xpath>
        </field>
    </record>

  </data>
</odoo>
