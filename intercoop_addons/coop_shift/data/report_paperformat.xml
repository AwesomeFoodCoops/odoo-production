<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Custom Paper Format for Wall Chart Ftop Report -->
    <record id="paperformat_wallchart_ftop" model="report.paperformat">
        <field name="name">Wall Chart Ftop</field>
        <field eval="True" name="default"/>
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">20</field>
        <field name="margin_bottom">23</field>
        <field name="margin_left">7</field>
        <field name="margin_right">7</field>
        <field eval="False" name="header_line"/>
        <field name="header_spacing">35</field>
        <field name="dpi">90</field>
    </record>
</odoo>