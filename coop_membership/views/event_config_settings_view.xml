<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_form1" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.coop.membership</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="event.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <div name="event_settings_website" position="after">
                <div class="col-12 col-lg-6 o_setting_box">
                    <div class="o_setting_left_pane"/>
                    <!--<div class="o_setting_right_pane">-->
                    <!--<label for="seats_max"/>-->
                    <!--<field name="seats_max"/>-->
                    <!--</div>-->
                </div>
                <div class="col-12 col-lg-6 o_setting_box">
                    <div class="o_setting_right_pane">
                        <div class="content-group">
                            <div class="row">
                                <label for="description" class="col-lg-3 o_light_label"/>
                                <field name="description"/>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-12 col-lg-6 o_setting_box">
                    <div class="o_setting_right_pane">
                        <div class="content-group">
                            <div class="mt16">
                                <label for="notice"/>
                                <field name="notice"/>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </field>
    </record>
</odoo>
