msgid ""
"\n"
"<p>Cher ${object.src_registration_id.partner_id.name}</p>\n"
"<p>${object.des_registration_id.partner_id.name} vous propose de remplacer "
"votre service du ${user.get_time_by_user_lang(object.src_registration_id."
"date_begin, ['%A %d %B %Y'])[0].lower()} à ${user."
"get_time_by_user_lang(object.src_registration_id.date_begin, ['%Hh%M'])[0]."
"lower()} par :</p>\n"
