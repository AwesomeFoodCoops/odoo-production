        <field name="inherit_id"
               ref="purchase_package_qty.product_supplierinfo_tree_view_package" />
        <field name="arch" type="xml">
            <field name="package_qty" position="before">
                <field name="min_nb_of_package"/>
            </field>
        </field>
    </record>
