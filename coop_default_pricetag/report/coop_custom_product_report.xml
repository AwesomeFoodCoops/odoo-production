<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <report
        id="product_print_category.pricetag"
        model="report.pricetag"
        string="Pricetags"
        report_type="qweb-pdf"
        name="coop_default_pricetag.report_pricetag"
        file="coop_default_pricetag.report_pricetag"
        attachment_use="False"
    />

    <record id="product_print_category.pricetag" model="ir.actions.report">
        <field name="paperformat_id" ref="paperformat_pricetag"/>
        <field name="attachment_use" eval="0"/>
    </record>

    <report
        id="pricetag_barcode"
        model="report.pricetag"
        string="Pricetags with Barcode"
        report_type="qweb-pdf"
        name="coop_default_pricetag.report_pricetag_barcode"
        file="coop_default_pricetag.report_pricetag_barcode"
        attachment_use="False"
    />

    <record id="pricetag_barcode" model="ir.actions.report">
        <field name="paperformat_id" ref="paperformat_pricetag"/>
        <field name="attachment_use" eval="0"/>
    </record>

</odoo>
