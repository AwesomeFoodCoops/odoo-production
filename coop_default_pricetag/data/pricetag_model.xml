<odoo>

    <record id="pricetag_model_default" model="pricetag.model">
        <field name="name">Default pricetag model</field>
        <field name="pricetag_paperformat_id" ref="coop_default_pricetag.paperformat_pricetag"/>
        <field name="report_model">coop_default_pricetag.report_pricetag</field>
    </record>

    <record id="pricetag_model_barcode" model="pricetag.model">
        <field name="name">Default pricetag model with barcode</field>
        <field name="pricetag_paperformat_id" ref="coop_default_pricetag.paperformat_pricetag"/>
        <field name="report_model">coop_default_pricetag.report_pricetag_barcode</field>
    </record>

    <record id="pricetag_model_vegetables" model="pricetag.model">
        <field name="name">Vegetables pricetag model</field>
        <field name="pricetag_paperformat_id" ref="coop_default_pricetag.paperformat_pricetag_vegetables"/>
        <field name="report_model">coop_default_pricetag.report_pricetag_vegetables</field>
    </record>

</odoo>
