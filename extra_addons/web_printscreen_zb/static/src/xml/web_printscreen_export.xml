<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

 <t t-extend="ListView.buttons">
       	<t t-jquery=".oe_list_buttons" t-operation="append">
       		<t t-if="widget.dataset.context.show_export_button or false">
		        <button class="btn btn-sm btn-default" type="button">
		            <a class="oe_bold oe_list_button_import_pdf"
			                   id="button_export_pdf"
			                   href="#">PDF</a>
		            <a class="oe_bold oe_list_button_import_excel"
			                   id="button_export_excel"
			                   href="#">Excel</a>
			    </button>
			</t>
       </t>
 </t>

</templates>
