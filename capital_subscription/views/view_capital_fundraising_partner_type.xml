<odoo>

    <record model="ir.ui.view" id="view_capital_fundraising_partner_type_form">
        <field name="model">capital.fundraising.partner.type</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="view_capital_fundraising_partner_type_tree">
        <field name="model">capital.fundraising.partner.type</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record model="ir.ui.view" id="view_capital_fundraising_partner_type_search">
        <field name="model">capital.fundraising.partner.type</field>
        <field name="arch" type="xml">
            <search>
                <field name="name" />
            </search>
        </field>
    </record>

</odoo>
