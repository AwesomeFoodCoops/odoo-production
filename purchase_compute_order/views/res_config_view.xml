<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_cpo_config_settings" model="ir.ui.view">
        <field name="name">cpo settings</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id"
               ref="purchase.res_config_settings_view_form_purchase"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@data-key='purchase']" position="inside">
                <h2>Purchase Compute Order</h2>
                <div class="row mt16 o_settings_container">
                    <label for="default_valid_psi"/>
                    <field name="default_valid_psi" widget="radio"/>
                </div>
            </xpath>
        </field>
    </record>
</odoo>
